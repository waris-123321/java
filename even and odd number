import java.util.Scanner;

public class array_even_and_odd {
    public static void main(String[] args) {
        int array[]=new int[8];
        Scanner s= new Scanner(System.in);
        int even=0;
        int odd=1;
        for(int i=0;i<8;i++){
            int n= s.nextInt();

            if(n%2==0){
               array[even]=n;
               even+=2;
            }
            if(n%2!=0){
                array[odd]=n;
                odd+=2;
            }
        }
    }
}
